{
  "name": "co2birthdate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf out/ .next/ node_modules/.cache",
    "build": "next build",
    "export": "next export && echo > out/.nojekyll",
    "prestart": "run-s clean build export",
    "start": "serve -l 3000 out",
    "predeploy": "run-s prestart",
    "deploy": "git add out/ && git commit -m \"Deploy to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "dev": "next",
    "test": "echo 'Error: no test specified'"
  },
  "dependencies": {
    "@material-ui/core": "^5.0.0-alpha.11",
    "framer-motion": "^2.7.7",
    "next": "^9.5.3",
    "nprogress": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2"
  }
}
